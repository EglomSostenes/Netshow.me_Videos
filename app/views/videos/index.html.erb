<p id="notice"><%= notice %></p>

<h1>ALL</h1> <br>

<table class="table table-striped table-hover">
  <thead class="thead-light">
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th>Views</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @videos.each do |video| %>
      <tr>
        <td><%= link_to video.name, video %></td>
        <td><%= video.url %></td>
        <td><%= video.score %></td>
        <td><%= link_to 'Play', play_video_path(video), class: '', title: "Play" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>




